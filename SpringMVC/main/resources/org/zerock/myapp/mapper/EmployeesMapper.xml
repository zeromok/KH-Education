<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
        namespace="org.zerock.myapp.mapper.EmployeesMapper">
    <select id="getAllEmployeesNames"
            resultType="java.lang.String">

        SELECT last_name
        FROM employees
        WHERE employee_id > 0

    </select>
</mapper>